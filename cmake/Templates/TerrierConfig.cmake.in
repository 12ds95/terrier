# Config file for the Terrier package.
#
# After successful configuration the following variables
# will be defined:
#
#   Terrier_INCLUDE_DIRS - Terrier include directories
#   Terrier_LIBRARIES    - libraries to link against
#   Terrier_DEFINITIONS  - a list of definitions to pass to compiler
#

# Compute paths
get_filename_component(Terrier_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)
set(Terrier_INCLUDE_DIRS "@Terrier_INCLUDE_DIRS@")

@Terrier_INSTALL_INCLUDE_DIR_APPEND_COMMAND@

# Our library dependencies
if(NOT TARGET terrier AND NOT terrier_BINARY_DIR)
  include("${Terrier_CMAKE_DIR}/TerrierTargets.cmake")
endif()

# List of IMPORTED libs created by TerrierTargets.cmake
set(Terrier_LIBRARIES terrier)

# Definitions
set(Terrier_DEFINITIONS "@Terrier_DEFINITIONS@")
